def words_mixer(word_list):     new_word= []     word_list.sort()     while len(word_list) > 5:             new_word.append(word_list.pop(-5))             new_word.append(word_list.pop(0))             new_word.append(word_list.pop())     return (new_word)   word = input("enter a string: ") word_list = word.split(" ") for i in range(len(word_list)):     if len(word_list[i]) <= 3:         word_list[i] = word_list[i].lower()     elif len(word_list[i]) >= 7:         word_list[i] = word_list[i].upper()  words_mixer(word_list) new_word = ' '.join(word_list) print(new_word)
